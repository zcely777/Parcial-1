{
    "dev": {
        "app_function": "lambda_scraper.download_html",
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "default",
        "project_name": "punto1",
        "runtime": "python3.10",
        "s3_bucket": "zappa-casas-aa",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
          {
            "function": "lambda_scraper.download_html",
            "expression":  "cron(0 9 ? * MON *)"
          }
        ]
    },
    
    "dev2": {
        "app_function": "lambda_parser.lambda_handler",
        "aws_region": "us-east-1",
        "profile_name": "default",
        "project_name": "punto1",
        "runtime": "python3.10",
        "s3_bucket": "zappa-casas-oo",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        "events": [
          {
            "event_source": {
                "arn": "arn:aws:s3:::landing-casas-c",
                "events": ["s3:ObjectCreated:*"]
            },
            "function": "lambda_parser.lambda_handler"
          }
        ]
    }
}
